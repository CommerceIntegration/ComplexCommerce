@model IEnumerable<ComplexCommerce.Business.Globalization.ProductLocaleInfo>

<div id="languages">
    <ul>
    @foreach (var item in Model) {
        var routeValues = ViewContext.RouteData.Values;
        routeValues["localeId"] = item.LocaleId;
        @Html.RouteLink(item.NativeName, "Product", routeValues)
    }
    </ul>
</div>